{
  "name": "tictactalk",
  "version": "1.0.0",
  "description": "tictactalk",
  "main": "dist/main.js",
  "author": "fabyday",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "webpack --config webpack.main.js && webpack --config webpack.preload.js && webpack --config webpack.renderer.js",
    "start": "yarn build && electron .",
    "start:web": "webpack --config webpack.renderer.js",
    "dist": "yarn build && electron-builder",
    "dist:win": "yarn build && electron-builder --win",
    "dist:zip": "yarn build && electron-builder --win --dir",
    "test": "npx tsx --tsconfig tsconfig.json tests/voice-test-bot.ts",
    "test:watch": "cd tests && npx tsx --watch --tsconfig tsconfig.json voice-test-bot.ts",
    "test:server": "npx tsx --tsconfig tsconfig.json tests/run-test-server.ts",
    "test:server-only": "npx tsx --tsconfig tsconfig.json tests/voice-test-server.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@headlessui/react": "^2.2.1",
    "dgram": "^1.0.1",
    "electron-log": "^5.4.3",
    "framer-motion": "^12.6.5",
    "lucide-react": "^0.487.0",
    "mediasoup-client": "^3.15.6",
    "opusscript": "^0.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "socket.io-client": "^4.8.1",
    "tailwind-scrollbar": "^4.0.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@storybook/addon-docs": "9.0.16",
    "@storybook/addon-onboarding": "9.0.16",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/addon-styling-webpack": "2.0.0",
    "@storybook/addon-webpack5-compiler-swc": "3.0.0",
    "@storybook/react-webpack5": "^9.1.5",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "autoprefixer": "^10.4.21",
    "css-loader": "^7.1.2",
    "electron": "^35.1.5",
    "electron-builder": "24.9.1",
    "electron-rebuild": "^3.2.9",
    "fluent-ffmpeg": "^2.1.3",
    "html-webpack-plugin": "^5.6.3",
    "libopus.js": "^0.0.1",
    "node-opus": "^0.3.3",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.2.0",
    "storybook": "9.0.16",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.3",
    "ts-loader": "^9.5.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "webm-writer": "^1.0.0",
    "webpack": "^5.99.5",
    "webpack-cli": "^6.0.1"
  },
  "build": {
    "appId": "com.tictactalk.app",
    "productName": "TicTacTalk",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "images/icon.jpg"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "TicTacTalk"
    }
  }
}
